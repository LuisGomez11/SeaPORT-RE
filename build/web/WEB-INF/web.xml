<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>miServlet</servlet-name>
        <servlet-class>Servlet.ControlServicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlProveedores</servlet-name>
        <servlet-class>Servlet.ControlProveedores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlServicios</servlet-name>
        <servlet-class>Servlet.ControlServicios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlAsignacion</servlet-name>
        <servlet-class>Servlet.ControlAsignacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlEntidad</servlet-name>
        <servlet-class>Servlet.ControlEntidad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlServ</servlet-name>
        <servlet-class>Servlet.ControlServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>miServlet</servlet-name>
        <url-pattern>/miServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlProveedores</servlet-name>
        <url-pattern>/ControlProveedores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlServicios</servlet-name>
        <url-pattern>/ControlServicios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlAsignacion</servlet-name>
        <url-pattern>/ControlAsignacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlEntidad</servlet-name>
        <url-pattern>/ControlEntidad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlServ</servlet-name>
        <url-pattern>/ControlServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
